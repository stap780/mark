<div class="space-y-4">
  <div>
    <div class="text-sm text-gray-500">API key</div>
    <div class="font-medium"><%= @insale.api_key %></div>
  </div>
  <div>
    <div class="text-sm text-gray-500">API password</div>
    <div class="font-medium"><%= @insale.api_password %></div>
  </div>
  <div>
    <div class="text-sm text-gray-500">API link</div>
    <div class="font-medium"><%= @insale.api_link %></div>
  </div>
  <div class="space-x-2">
  <%= link_to "Edit", edit_account_insale_path(account_id: current_account, id: @insale), class: "inline-flex items-center px-3 py-1 bg-violet-600 text-white rounded-md hover:bg-violet-700" %>
  <%= link_to "Back", account_insales_path(account_id: current_account), class: "inline-flex items-center px-3 py-1 bg-gray-100 text-gray-700 rounded-md hover:bg-gray-200" %>
  </div>
</div>
