<%= turbo_frame_tag dom_id(mailganer) do %>
  <div class="grid grid-cols-[1fr_1fr_1fr_150px] gap-4 p-4 items-center border-b">
    <div class="text-sm text-gray-900"><%= mailganer.api_key %></div>
    <div class="text-sm text-gray-900"><%= mailganer.smtp_login %></div>
    <div class="text-sm text-gray-900"><%= mailganer.api_key_web_portal %></div>
    <div class="flex justify-end items-center space-x-2">
      <%= link_to_edit edit_account_mailganer_path(account_id: current_account, id: mailganer), data: { turbo_frame: :offcanvas } %>
      <%= link_to_show account_mailganer_path(current_account, mailganer), data: { turbo_frame: :offcanvas } %>
    </div>
  </div>
<% end %>


